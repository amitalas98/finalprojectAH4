survival_rate <- function(class, dataframe) {
classL <- dataframe[dataframe$Class == class,]
adult <- classL[class$Age == "Adult",]
male <- classL[adult$Sex == "Male",]
totalS <- classL[classL$Survived == "Yes",]
numberOfMale <- sum(male$Freq)
numberOfAll <- sum(classL$Freq)
numberOfWC <- sum(classL$Freq) - sum(male$Freq)
numberOfMaleS <- male[male$Survived == "Yes",]
numberOfMaleSR <- floor((sum(numberOfMaleS$Freq) / numberOfMale) * 100)
numberOfWCS <- totalS - numberOfMaleS
numberOfWCSR <- floor((sum(numberOfWCS$Freq) / numberOfWC) * 100)
return("Of " + class + " class, " + numberOfWCSR + "% of women and children survived and " + numberOfMaleSR +"% of men survived.")
}
